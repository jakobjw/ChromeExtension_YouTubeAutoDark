{
 "name": "YouTube AutoDark",
 "version": "0.4",
 "manifest_version" : 2,
 "description": "Automatically enables or disables YouTube Dark theme based on the current time",
 "content_scripts": [
	 {
		 "matches": ["http://*.youtube.com/*", "https://*.youtube.com/*"],
		 "js": ["main.js"],
		 "run_at": "document_idle"
	 }
 ],
 "permissions": [
    "storage"
  ],
 "options_ui": {
    "page": "options.html",
	"open_in_tab": false
	},
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
 }
}
